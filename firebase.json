{
  "hosting": {
    "site": "wallet-auth",
    "public": "public",
    "rewrites": [
      {
        "source": "/widget",
        "destination": "/widget.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
